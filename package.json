{
  "name": "littlejsengine",
  "version": "1.4.0",
  "description": "LittleJS - Tiny and Fast HTML5 Game Engine",
  "main": "engine/engine.all.js",
  "exports": {
    ".": {
      "require": "./engine/engine.all.js",
      "import": "./engine/engine.all.module.js"
    },
    "./release": {
      "require": "./engine/engine.all.release.js",
      "import": "./engine/engine.all.release.module.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KilledByAPixel/LittleJS.git"
  },
  "keywords": [
    "HTML5",
    "JavaScript",
    "game",
    "engine",
    "library",
    "JS13K",
    "webgl"
  ],
  "author": "Frank Force",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KilledByAPixel/LittleJS/issues"
  },
  "homepage": "https://github.com/KilledByAPixel/LittleJS",
  "scripts": {
    "check-filesize": "filesize.bat game.zip",
    "build:esm": "node --experimental-modules build-esm.mjs",
    "build": "build.bat && npm run build:esm && npm run check-filesize"
  },
  "dependencies": {
    "ect-bin": "^1.4.1",
    "google-closure-compiler": "^20221102.0.0",
    "roadroller": "^2.1.0",
    "typescript": "^4.8.4",
    "uglify-js": "^3.17.4"
  }
}
